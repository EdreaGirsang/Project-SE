<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DDDDDD"
    tools:context=".PengurusLogin">
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/quantico"
        android:text="MANAKOS"
        android:textColor="@color/black"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/sloganOwner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/quando"
        android:text="New Way to Manage Your Property"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="Your Kos"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/OwnId"
        app:layout_constraintEnd_toEndOf="@+id/OwnId"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/OwnId"
        tools:fontFamily="@font/quando"/>

    <EditText
        android:id="@+id/OwnId"
        android:layout_width="285dp"
        android:layout_height="56dp"
        android:layout_marginTop="288dp"
        android:background="@drawable/loginbtn"
        android:backgroundTint="@color/white"
        android:elevation="15dp"
        android:textColor="@color/black"
        android:fontFamily="@font/quantico"
        android:hint="Owner Id"
        android:padding="12dp"
        android:textStyle="italic"
        android:shadowColor="@color/black"
        android:textColorHint="@color/gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <EditText
        android:id="@+id/KosId"
        android:textColor="@color/black"
        android:layout_width="283dp"
        android:layout_height="56dp"
        android:layout_marginTop="20dp"
        android:textStyle="italic"
        android:background="@drawable/rounded2"
        android:backgroundTint="@color/white"
        android:elevation="15dp"
        android:fontFamily="@font/quantico"
        android:hint="@string/KID"
        android:padding="12dp"
        android:shadowColor="@color/black"
        android:textColorHint="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/OwnId"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/OwnId"
        app:layout_constraintTop_toBottomOf="@id/OwnId"
        app:layout_constraintVertical_bias="0.0"/>

    <Button
        android:id="@+id/LgnBtn"
        android:layout_width="222dp"
        android:layout_height="48dp"
        android:layout_marginBottom="128dp"
        android:background="@drawable/loginbtn"
        android:backgroundTint="@color/white"
        android:elevation="7dp"
        android:fontFamily="@font/quando"
        android:text="Login"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/KosId"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="@+id/KosId" />
