<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="360dp"
    android:layout_height="400dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:background="#D9D9D9"
    android:padding="5dp">

    <TextView
        android:id="@+id/compl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Completed Report"
        android:textStyle="bold"
        android:layout_marginTop="35dp"
        android:textSize="30dp"
        android:fontFamily="@font/quando"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor="@color/black"/>

    <View
        android:id="@+id/box"
        android:layout_width="320dp"
        android:layout_height="240dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="110dp"
        android:background="@drawable/rounded4"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv11"
        android:layout_width="300dp"
        android:layout_height="230dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="@id/box"
        app:layout_constraintBottom_toBottomOf="@id/box"
        app:layout_constraintLeft_toLeftOf="@id/box"
        app:layout_constraintRight_toRightOf="@id/box"/>


</androidx.constraintlayout.widget.ConstraintLayout>